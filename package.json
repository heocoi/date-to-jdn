{
  "name": "date-to-jdn",
  "version": "0.1.0",
  "description": "A npm package for converting Gregorian calendar date to Julian Day Number (JDN).",
  "license": "MIT",
  "repository": "heocoi/date-to-jdn",
  "author": {
    "name": "heocoi",
    "email": "anhphong.pham@gmail.com",
    "url": "https://github.com/heocoi"
  },
  "type": "module",
  "exports": "./index.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "test": "xo && ava"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "Gregorian calendar date",
    "Julian calendar",
    "Julian Day Number"
  ],
  "dependencies": {
    "timezone-mock": "^1.3.6"
  },
  "devDependencies": {
    "ava": "^5.3.0",
    "xo": "^0.54.2"
  },
  "xo": {
    "space": true,
    "rules": {
      "unicorn/empty-brace-spaces": "off",
      "unicorn/numeric-separators-style": "off",
      "no-mixed-operators": "off"
    }
  }
}
